    "@vitest/coverage-v8": "^2.1.8",
    "daisyui": "^5.5.14",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^15.0.0",
    "happy-dom": "^20.0.11",
    "prettier": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.21",
    "vitest": "^2.1.8"
  }
